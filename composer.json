    "version": "1.3",
    "type": "library",
    "keywords": [
        "php"
        ,"statemachine",
        "fsm",
        "finite-state-machine",
        "state"
    ],
    "homepage": "https://github.com/EFTEC/StateMachineOne",
    "authors": [
        {
            "name": "Jorge Castro Castillo",
            "homepage": "https://github.com/EFTEC/"
        }
    ],
    "minimum-stability": "beta",
    "license": "LGPL-3.0",
    "require": {
        "php": ">=7.0",
        "composer/installers": "^1",
        "eftec/daoone": "^3.19",
        "ext-ctype": "*"
    },
    "require-dev": {
      "phpunit/phpunit": "^5.7 || ^6.5"
    },
    "autoload": {
      "psr-4": {
        "eftec\\": "vendor/eftec/",
        "eftec\\statemachineone\\": "lib/"
      }
