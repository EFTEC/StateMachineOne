{
    "name": "eftec/statemachineone",
    "description": "A state Machine library for business processes",
    "type": "library",
    "keywords": [
        "php"
        ,"statemachine",
        "fsm",
        "finite-state-machine",
        "state"
    ],
    "homepage": "https://github.com/EFTEC/StateMachineOne",
    "authors": [
        {
            "name": "Jorge Castro Castillo",
            "homepage": "https://github.com/EFTEC/"
        }
    ],
    "minimum-stability": "beta",
    "license": "LGPL-3.0",
    "require": {
        "php": ">=7.2",
        "eftec/pdoone": "^3.0",
        "eftec/minilang": "^2.20.2",
        "ext-ctype": "*",
        "ext-json": "*",
        "eftec/documentstoreone": "^1.23"
    },
    "require-dev": {
      "phpunit/phpunit": ">5.7"
    },
    "autoload": {
      "psr-4": {
        "eftec\\": "vendor/eftec/",
        "eftec\\statemachineone\\": "lib/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "eftec\\tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
